<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Knight_Tour</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <PackageIcon>knight.png</PackageIcon>
  </PropertyGroup>

  <ItemGroup>
    <Resource Include="images\knight.png" />
  </ItemGroup>

  <ItemGroup>
    <None Update="images\knight.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
